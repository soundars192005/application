using System;

class StoreSalesAnalysis
{
    static void Main()
    {
        Console.Write("Enter number of days in the month: ");
        int days = int.Parse(Console.ReadLine());
        int[] sales = new int[days];
        int[] backupSales = new int[days];
        Console.WriteLine("\nEnter sales for each day:");
        for (int i = 0; i < days; i++)
        {
            Console.Write($"Day {i + 1}: ");
            sales[i] = int.Parse(Console.ReadLine());
        }
        Array.Copy(sales, backupSales, days);
        for (int i = 0; i < days; i++)
        {
            Console.WriteLine($"Day {i + 1}: {sales[i]}");
        }
        int[] sortedSales = new int[days];
        Array.Copy(sales, sortedSales, days);
        Array.Sort(sortedSales);
        foreach (int s in sortedSales)
        {
            Console.WriteLine(s);
        }
        int total = 0;
        int highest = int.MinValue;
        int lowest = int.MaxValue;
        for (int i = 0; i < days; i++)
        {
            total += sales[i];
            if (sales[i] > highest) highest = sales[i];
            if (sales[i] < lowest) lowest = sales[i];
        }
        Console.WriteLine($"Total Sales: {total}");
        Console.WriteLine($"Highest Sales: {highest}");
        Console.WriteLine($"Lowest Sales: {lowest}");
        Console.Write("\nEnter a sales value to search: ");
        int searchValue = int.Parse(Console.ReadLine());
        bool found = Array.Exists(sales, x => x == searchValue);
        if (found)
            Console.WriteLine($"Sales value {searchValue} FOUND in the data.");
        else
            Console.WriteLine($"Sales value {searchValue} NOT FOUND in the data.");
        Console.WriteLine("\nEnter another set of sales data to compare:");
        int[] userSales = new int[days];
        for (int i = 0; i < days; i++)
        {
            Console.Write($"Day {i + 1}: ");
            userSales[i] = int.Parse(Console.ReadLine());
        }
        bool equal = true;
        for (int i = 0; i < days; i++)
        {
            if (userSales[i] != sales[i])
            {
                equal = false;
                break;
            }
        }
        if (equal)
            Console.WriteLine("Both sales datasets are EQUAL.");
        else
            Console.WriteLine("The sales datasets are NOT EQUAL.");
    }
}
